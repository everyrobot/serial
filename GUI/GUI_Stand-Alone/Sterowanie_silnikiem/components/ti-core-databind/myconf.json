{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
    	"exclude": ["src/WidgetBindingManager.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/inheritDoc", "plugins/relativeLinks"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false,
			"includeDate": false
        }
    },
    "recurseDepth": 1
}